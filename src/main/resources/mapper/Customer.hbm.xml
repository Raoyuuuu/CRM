<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fjut.domain.Customer" table="cst_customer">
        <id name="cust_id" column="cust_id">
            <generator class="native"/>
        </id>

        <property name="cust_name" column="cust_name" />
        <property name="cust_phone" column="cust_phone" />
        <property name="cust_email" column="cust_email" />

        <many-to-one name="baseDictSource" class="com.fjut.domain.BaseDict" column="cust_source"/>
        <many-to-one name="baseDictIndustry" class="com.fjut.domain.BaseDict" column="cust_industry"/>
        <many-to-one name="baseDictLevel" class="com.fjut.domain.BaseDict" column="cust_level"/>
    </class>
</hibernate-mapping>